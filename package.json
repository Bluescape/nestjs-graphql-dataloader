{
  "name": "@bluescape/nestjs-graphql-dataloader",
  "version": "2.0.0",
  "private": false,
  "description": "NestJS GraphQL Dataloader",
  "license": "MIT",
  "repository": "https://github.com/bluescape/nestjs-graphql-dataloader",
  "author": "Mark Everett",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && tsc -p tsconfig.json",
    "prepare": "tsc -p tsconfig.json",
    "prestart": "rm -rf ./example/dist",
    "start": "nest start example/src/main.ts --watch --path=example/tsconfig.json",
    "test": "jest"
  },
  "keywords": [
    "nestjs",
    "graphql",
    "dataloader"
  ],
  "publishConfig": {
    "registry": "https://artifactory.common.bluescape.com/api/npm/bluescape-common"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.0.0",
    "graphql": "^16.0.0",
    "rxjs": "^7.0.0"
  },
  "dependencies": {
    "dataloader": "^2.2.2"
  },
  "devDependencies": {
    "@apollo/gateway": "2.0.2",
    "@nestjs/cli": "^10.0.3",
    "@nestjs/common": "^10.0.3",
    "@nestjs/core": "^10.0.3",
    "@nestjs/graphql": "^12.0.3",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.3",
    "@nestjs/typeorm": "^10.0.0",
    "@types/express": "4.17.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.11.8",
    "@types/supertest": "^2.0.8",
    "apollo-server-express": "^3.7.0",
    "graphql": "^16.6.0",
    "jest": "^27.1.0",
    "prettier": "^2.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.5.4",
    "sqlite3": "^5.0.0",
    "supertest": "^5.0.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "^0.3.0",
    "typescript": "^4.4.2"
  },
  "types": "dist/index.d.ts",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.ts$",
    "collectCoverageFrom": [
      "src/index.ts"
    ]
  }
}
